{
  "name": "dev-reports-api",
  "version": "0.0.1",
  "description": "REST API for managing developer reports",
  "author": "Jose M Malaguilla",
  "contactEmail": "josemalaguilla@hotmail.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "lint:check": "eslint",
    "lint:fix": "eslint --fix",
    "libs:check": "npm-check-updates",
    "libs:update": "npm-check-updates -u",
    "security:auth": "snyk auth",
    "security:check": "snyk test",
    "build": "nest build",
    "start": "node dist/src/main",
    "start:debug": "node --inspect=0.0.0.0:9229 dist/src/main",
    "dev": "nest start",
    "dev:watch": "nest start --watch",
    "build:docker": "docker compose -f docker/docker-compose.yml --profile all build",
    "start:docker": "docker compose -f docker/docker-compose.yml --profile api up -d",
    "stop:docker": "docker compose -f docker/docker-compose.yml --profile api down",
    "docs:swagger": "ts-node -r tsconfig-paths/register tools/docs/swagger/swagger-generate.ts",
    "test": "jest --config ./config/jest.config.unit.ts",
    "test:watch": "jest --config ./config/jest.config.unit.ts --watchAll --detectOpenHandles",
    "test:int": "node config/scripts/run-e2e.js ./config/jest.config.integration.ts --detectOpenHandles",
    "test:e2e": "node config/scripts/run-e2e.js ./config/jest.config.e2e.ts --detectOpenHandles",
    "test:cov": "node config/scripts/run-e2e.js ./config/jest.config.coverage.ts --coverage --detectOpenHandles",
    "docker:up": "docker compose -f docker/docker-compose.yml --profile dependencies up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml --profile all down",
    "docker:restart": "npm run docker:down && npm run docker:up",
    "wait-for-services": "node docker/scripts/wait-startup.js",
    "clean": "rm -rf node_modules package-lock.json dist",
    "prepare": "husky",
    "commit": "cz"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.810.0",
    "@aws-sdk/client-sns": "^3.810.0",
    "@aws-sdk/client-sqs": "^3.810.0",
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/microservices": "^11.1.1",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@ssut/nestjs-sqs": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.5.0",
    "joi": "^17.13.3",
    "mime-types": "^3.0.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@faker-js/faker": "^9.8.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.1",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.18",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.0.0",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "snyk": "^1.1297.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.3",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
